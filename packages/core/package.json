{
  "name": "@agentpass/core",
  "version": "0.1.0",
  "type": "module",
  "description": "Shared core library: crypto, passport, vault, types",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./crypto": {
      "import": "./dist/crypto/index.js",
      "types": "./dist/crypto/index.d.ts"
    },
    "./passport": {
      "import": "./dist/passport/index.js",
      "types": "./dist/passport/index.d.ts"
    },
    "./vault": {
      "import": "./dist/vault/index.js",
      "types": "./dist/vault/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "better-sqlite3": "^11.8.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  }
}
